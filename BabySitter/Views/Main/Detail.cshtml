@model BabySitter.Models.InformationTravel

    @{
        ViewBag.Title = "Chi tiết";
        Layout = "~/Views/Shared/_Layout.cshtml";
    }
<meta name="description" content="@Model.detail" />
<meta name="description" content="@Model.title" />
<meta name="description" content="@Model.img" />
<div id="heading-breadcrumbs">
    <div class="container">
        <div class="row d-flex align-items-center flex-wrap">
            <div class="col-md-7">
                <h1 class="h2">Information Travel</h1>
            </div>
            <div class="col-md-5">
                <ul class="breadcrumb d-flex justify-content-end">
                    <li class="breadcrumb-item"><a href="index.html">Home</a></li>
                    <li class="breadcrumb-item active">Information Travel / @Model.title</li>
                </ul>
            </div>
        </div>
    </div>
</div>

<div id="content">
    <div class="container">
        <section class="bar no-padding-bottom">
            <div class="row">
                <div class="col-lg-9">
                    <div class="heading">
                        <h2>@Model.title</h2>
                    </div>
                    @Html.Raw(@Model.detail)
                    <div class="fb-comments" data-href="http://projectbabysitter.somee.com/@Model.meta-@Model.id_InformationTravel" data-width="855" data-numposts="5"></div>
                </div>
                <div class="col-md-3">
                    <div class="panel panel-default sidebar-menu">
                        <div class="panel-heading">
                            <h3 class="h4 panel-title">Categories</h3>
                        </div>
                        <div class="box-image">
                            <div class="image">
                                <img src="~/Content/img/product2.jpg" alt="" class="img-fluid">
                                <div class="overlay d-flex align-items-center justify-content-center">
                                    <div class="content">
                                        <div class="name mb-small">
                                            <h3><a href="portfolio-detail.html" class="color-white">Portfolio item</a></h3>
                                        </div>
                                        <div class="text">
                                            <p class="d-none">Pellentesque habitant morbi tristique senectus et netus et malesuada</p>
                                            <p class="buttons"><a href="portfolio-detail.html" class="btn btn-template-outlined-white">View</a><a href="#" class="btn btn-template-outlined-white">Website</a></p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
</div>